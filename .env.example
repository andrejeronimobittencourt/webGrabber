# Environment Variables for webGrabber

# Server Configuration
PORT=3000

# Logging
LOG_LEVEL=info  # Options: error, warn, info, debug
CONSOLE_LOG_LEVEL=error  # Console log level (usually less verbose than file logs)

# Rate Limiting (Server Mode)
GRABBER_RATE_LIMIT_WINDOW_MS=900000  # Time window in milliseconds (default: 15 minutes)
GRABBER_RATE_LIMIT_MAX_REQUESTS=100  # Maximum requests per window (default: 100)

# Custom Variables (Add your own with GRABBER_ prefix)
# These will be available in your grabs via {{VARIABLE_NAME}}
# Example:
# GRABBER_USERNAME=myusername
# GRABBER_PASSWORD=mypassword
# GRABBER_API_KEY=yourapikey
